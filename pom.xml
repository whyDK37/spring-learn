<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://maven.apache.org/POM/4.0.0"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">


  <build>
    <plugins>
      <plugin>
        <artifactId>spring-boot-maven-plugin</artifactId>
        <groupId>org.springframework.boot</groupId>
      </plugin>
    </plugins>

  </build>
  <dependencies>
    <!--  self module-->
    <dependency>
      <artifactId>domain</artifactId>
      <groupId>me</groupId>
      <version>1.0</version>
    </dependency>

    <!-- spring framework -->
    <dependency>
      <artifactId>spring-context</artifactId>
      <groupId>org.springframework</groupId>
      <version>${spring.version}</version>
    </dependency>
  </dependencies>
  <artifactId>spring-learn</artifactId>
  <groupId>me</groupId>
  <modelVersion>4.0.0</modelVersion>
  <description>spring-learn project for Spring Boot</description>
  <name>spring-learn</name>


  <modules>
    <module>springboot-metrics</module>
    <module>spring-annotation</module>
    <module>springmvc-annotation</module>
    <module>servlet3.0</module>
    <module>spring-cloud-parent</module>
    <module>spring-boot-parent</module>
    <module>spring-data</module>
    <module>spring-framework</module>
    <module>domain</module>
  </modules>

  <packaging>pom</packaging>

  <properties>
    <build.file.encoding>UTF-8</build.file.encoding>
    <junit.version>5.6.1</junit.version>
    <maven.compiler.source>1.8</maven.compiler.source>

    <maven.compiler.target>1.8</maven.compiler.target>

    <maven.test.skip>true</maven.test.skip>
    <mysql.driver.version>8.0.13</mysql.driver.version>
    <scala-version>2.10</scala-version>

    <sentinel.version>1.6.3</sentinel.version>
    <servlet.version>3.1.0</servlet.version>

    <guava.version>29.0-jre</guava.version>
    <spark-version>2.0.2</spark-version>
    <spring-boot.version>2.2.0.RELEASE</spring-boot.version>
    <spring-cloud.version>2.2.0.RELEASE</spring-cloud.version>

    <spring.version>5.2.0.RELEASE</spring.version>
  </properties>


  <version>1.0</version>
</project>
